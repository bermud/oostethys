<?xml version="1.0" encoding="UTF-8"?>
<sml:SensorML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:sml="http://www.opengis.net/sensorML/1.0.1" xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:gml="http://www.opengis.net/gml" xmlns:swe="http://www.opengis.net/swe/1.0.1"
    xsi:schemaLocation="http://www.opengis.net/sensorML/1.0.1 http://schemas.opengis.net/sensorML/1.0.1/sensorML.xsd"
    version="1.0.1">
    
 

    <sml:member xlink:href="urn:mbari.org:ssds:device:m2">
        <!-- A System is an observatory, platform or sensor  -->
        <sml:System>
            <sml:identification>
                <sml:IdentifierList>
                    <sml:identifier name="Long Name">
                        <sml:Term
                            definition="http://mmisw.org/mmi/20080520/obs.owl#longName">
                            <sml:value>MBARI Mooring M2</sml:value>
                        </sml:Term>
                    </sml:identifier>
                    <sml:identifier name="ShortName">
                        <sml:Term
                            definition="http://mmisw.org/mmi/20080520/obs.owl#shortName">
                            <sml:value>M2</sml:value>
                        </sml:Term>
                    </sml:identifier>
                    <sml:identifier name="Identifier">
                        
                        <!-- unique identifier for this system. This is unique for each sos. Should be unique universally.  -->
                        <sml:Term
                            definition="http://mmisw.org/mmi/20080520/obs.owl#identifier">
                            <sml:value>urn:mbari.org:ssds:device:m2</sml:value>
                        </sml:Term>
                    </sml:identifier>

                    <sml:identifier name="Manufacturer Name">
                        <sml:Term
                            definition="http://mmisw.org/mmi/20080520/obs.owl#manufacturerName">

                            <sml:value/>
                        </sml:Term>
                    </sml:identifier>
                    <sml:identifier name="Model number ">
                        <sml:Term
                            definition="http://mmisw.org/mmi/20080520/obs.owl#modelNumber">
                            <sml:value/>
                        </sml:Term>
                    </sml:identifier>

                    <sml:identifier name="Serial number ">
                        <sml:Term
                            definition="http://mmisw.org/mmi/20080520/obs.owl#serialNumber">
                            <sml:value/>
                        </sml:Term>
                    </sml:identifier>
                </sml:IdentifierList>
            </sml:identification>
            <sml:classification>
                <sml:ClassifierList>
                    <sml:classifier name="System Type">
                        <!-- we are using rdf:type which says that this system type is an RDF class, or ontology class -->
                        <sml:Term definition="http://www.w3.org/1999/02/22-rdf-syntax-ns#type">
                            <sml:value>http://mmisw.org/mmi/20080516/platform#MooredBuoy</sml:value>
                        </sml:Term>
                    </sml:classifier>
                </sml:ClassifierList>
            </sml:classification>

            <!-- deployment information -->
            <sml:validTime>
                <gml:TimePeriod>
                    <gml:beginPosition>2008-04-11T20:06:20Z</gml:beginPosition>
                    <gml:endPosition>2008-05-29T22:06:20Z</gml:endPosition>
                </gml:TimePeriod>
            </sml:validTime>



            <!-- ================================================= -->
            <!--                  Relevant Contacts                -->
            <!-- ================================================= -->

            <!-- Data Provider  -->

            <sml:contact
                xlink:arcrole="http://mmisw.org/mmi/20080520/obs.owl#serviceProvider"
                xlink:title="MMI" xlink:href="http://marinemetadata.org">
                
                <sml:ResponsibleParty>
                    <sml:individualName>Luis Bermudez</sml:individualName>
                    <sml:organizationName>Marine Metadata Interoperability Initiative</sml:organizationName>
                    <sml:contactInfo>
                        <sml:address>
                            <sml:electronicMailAddress>bermudez@sura.org</sml:electronicMailAddress>
                        </sml:address>
                    </sml:contactInfo>
                </sml:ResponsibleParty>
            </sml:contact>

            <!-- Regional Association  -->

            <sml:contact
                xlink:arcrole="http://mmisw.org/mmi/20080520/obs.owl#regionalAssociation"
                xlink:title="CeNCOOS" xlink:href="http://www.cencoos.org/"/>

            <!--    principleInvestigator -->

            <sml:contact
                xlink:arcrole="http://mmisw.org/mmi/20080520/obs.owl#principalInvestigator"
                xlink:title="MBARI" xlink:href="http://www.mbari.org">
                <sml:ResponsibleParty>
                    <sml:individualName>Francisco Chavez</sml:individualName>
                    <sml:contactInfo>
                        <sml:address>
                            <sml:electronicMailAddress>chfr@mbari.org</sml:electronicMailAddress>
                        </sml:address>
                    </sml:contactInfo>
                </sml:ResponsibleParty>
            </sml:contact>


            <!-- ================================================= -->
            <!--                  containing  components           -->
            <!-- ================================================= -->
            <sml:components>
                <sml:ComponentList>
                    <sml:component name="" xlink:href="urn:mbari.org:ssds:device:1357">
                        <sml:System>
                            <sml:identification>
                                <sml:IdentifierList>
                                    <sml:identifier name="Long Name">
                                        <sml:Term
                                            definition="http://mmisw.org/mmi/20080520/obs.owl#longName">
                                            <sml:value>MicroCAT C-T (P optional) Recorder SBE 37-SMP
                                            </sml:value>
                                        </sml:Term>
                                    </sml:identifier>
                                    <sml:identifier name="Short Name">
                                        <sml:Term
                                            definition="http://mmisw.org/mmi/20080520/obs.owl#shortName">
                                            <sml:value>microcat-SBE37SMP</sml:value>
                                        </sml:Term>
                                    </sml:identifier>
                                    <sml:identifier name="Identifier">
                                        <!-- unique identifier for this system. This is unique for each sos. Should be unique universally.  -->
                                        <sml:Term
                                            definition="http://mmisw.org/mmi/20080520/obs.owl#identifier">
                                            <sml:value>urn:mbari.org:ssds:device:1357</sml:value>
                                        </sml:Term>
                                    </sml:identifier>

                                    <sml:identifier name="Manufacturer Name">
                                        <sml:Term
                                            definition="http://mmisw.org/mmi/20080520/obs.owl#manufacturerName">

                                            <sml:value>Seabird Electronics</sml:value>
                                        </sml:Term>
                                    </sml:identifier>
                                    <sml:identifier name="Model number ">
                                        <sml:Term
                                            definition="http://mmisw.org/mmi/20080520/obs.owl#modelNumber">
                                            <sml:value>SBE37SMP</sml:value>
                                        </sml:Term>
                                    </sml:identifier>

                                    <sml:identifier name="Serial number ">
                                        <sml:Term
                                            definition="http://mmisw.org/mmi/20080520/obs.owl#serialNumber">
                                            <sml:value>3937</sml:value>
                                        </sml:Term>
                                    </sml:identifier>
                                </sml:IdentifierList>
                            </sml:identification>

                            <sml:classification>
                                <sml:ClassifierList>
                                    <sml:classifier name="System Type">
                                        <!-- we are using rdf:type which says that this system type is an RDF class, or ontology class -->
                                        <sml:Term
                                            definition="http://www.w3.org/1999/02/22-rdf-syntax-ns#type">
                                            <sml:value>http://mmisw.org/mmi/20080529/sensor.owl#CTD</sml:value>
                                        </sml:Term>
                                    </sml:classifier>
                                </sml:ClassifierList>
                            </sml:classification>
                            <sml:outputs>
                                <sml:OutputList>
                                    <sml:output name="pointDataRecord">
                                        <swe:DataRecord
                                            definition="http://mmisw.org/mmi/20080520/obs.owl#pointDataRecord">
                                            <!-- uom use ucum units. More info: http://marinemetadata.org/vocabularies/refs/refsentries/ucumunits/view -->
                                            <swe:field name="latitude">
                                                <swe:Quantity
                                                  definition="urn:ogc:phenomenon:latitude:wgs84">
                                                  <swe:uom code="deg"/>
                                                </swe:Quantity>
                                            </swe:field>
                                            <swe:field name="longitude">
                                                <swe:Quantity
                                                  definition="urn:ogc:phenomenon:longitude:wgs84">
                                                  <swe:uom code="deg"/>
                                                </swe:Quantity>
                                            </swe:field>
                                            <swe:field name="depth">
                                                <!-- EPSG:6.5:5113 is reference frame for floating platforms -->
                                                <swe:Quantity definition="urn:ogc:phenomenon:depth" referenceFrame="urn:ogc:def:datum:EPSG:6.15:5113">
                                                    
                                                  <swe:uom code="m"/>
                                                </swe:Quantity>
                                            </swe:field>
                                            <swe:field name="">
                                                <swe:DataArray>
                                                  <swe:elementCount>
                                                  </swe:elementCount>
                                                  <swe:elementType
                                                  name="pointTimeSeriesDataRecord">
                                                  <swe:DataRecord
                                                  definition="http://mmisw.org/mmi/20080520/obs.owl#pointTimeSeriesDataRecord">
                                                  <swe:field name="time">
                                                  <swe:Time
                                                  definition="urn:ogc:phenomenon:time:iso8601"
                                                  />
                                                  </swe:field>
                                                  <swe:field name="Temperature">
                                                  <swe:Quantity
                                                  definition="http://mmisw.org/cf/20080530/var#sea_water_temperature">
                                                  <swe:uom code="Cel"> </swe:uom>
                                                    
                                                  </swe:Quantity>
                                                  </swe:field>

                                                  <swe:field name="Conductivity">
                                                  <swe:Quantity
                                                  definition="http://mmisw.org/cf/20080530/var#sea_water_electrical_conductivity">
                                                  <swe:uom code="S/m"/>
                                                  </swe:Quantity>
                                                  </swe:field>
                                                  <swe:field name="Salinity">
                                                  <swe:Quantity
                                                  definition="http://mmisw.org/cf/20080530/var#sea_water_salinity">
                                                  <swe:uom code="1"/>
                                                  </swe:Quantity>
                                                  </swe:field>
                                                  </swe:DataRecord>

                                                  </swe:elementType>
                                                </swe:DataArray>
                                            </swe:field>
                                        </swe:DataRecord>
                                    </sml:output>
                                </sml:OutputList>
                            </sml:outputs>
                        </sml:System>
                    </sml:component>
                </sml:ComponentList>
            </sml:components>

        </sml:System>


    </sml:member>

</sml:SensorML>
